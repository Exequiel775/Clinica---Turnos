<style type="text/css">
body{
        background-color: #dcdcdc;
    }
    .contenedor-tabla{
        background-color: #fff;
        padding-right: 20px;
        padding-left: 20px;
    }

    .turnos-paciente{
        margin-top: 10px;
        margin-right: 10px;
        margin-left: 10px;
    }

    .contenedor-tabla h1, h2, h3, h4, h5{
        font-family: 'PT Sans', sans-serif;
    }

    .lookup{
        padding-top: 20px;
        padding-bottom: 20px;
        background-color: #fff;
        padding-left: 20px;
        padding-right: 20px;
    }

    .tabla tr th{
        border: 1px solid #718093;
    }
    /*
    .tabla{
        background-color: #dcdde1;
    }

    .tabla tbody tr td{
        background-color: #fff;
    }
    */
    .tabla tbody{
        border:1px solid #000 !important;
    }

    .tabla tbody td{
        border:1px solid #718093;
    }

    .tabla tbody td{
        font-family: 'PT Sans', sans-serif;
    }

    .tabla thead{
        background-color: #273c75;
        color:#fff;
    }

    .datos-turno{
        background-color: #fff;
        margin-top: 10px;
        padding-right: 20px;
        padding-left: 20px;
        padding-top: 20px;
        padding-bottom: 20px;
    }

    .fila-seleccionada{
        background-color: #222f3e;
        color:#fff;
    }

    .fila-no-seleccionada{
        background-color: #fff;
        color:#000;
    }
</style>

